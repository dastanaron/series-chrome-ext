{
	"name": "Series Saver",
	"description": "This extension is remembering you notice about TV series",
	"version": "1.1",
	"manifest_version": 3,
	"icons": {
		"16": "./icons/icon_16.png",
		"48": "./icons/icon_48.png"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": "./icons/popup.png"
	},
	"permissions": [
		"activeTab",
		"storage"
	],
	"background": {
		"service_worker": "background.js"
	}
}
